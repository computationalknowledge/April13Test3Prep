#success tips for TEST:
#  Do all your work with Objects and Classes, and Method Calls
#   Your method calls should have good return types
# Start by writing a program that produces a Simple Output and Work your way up

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp16
{

    class Program {
            public static void Main(String []a) {
            ManageUserInput b = new ManageUserInput();
            b.acquireUserInput();
            Console.WriteLine(b);
        }
    }

    class ManageUserInput
    {
        public String acquireUserInput()
        {
            return Console.ReadLine();
        }

    }
}


